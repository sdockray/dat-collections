  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdockray/dat-collections/issues"
  },
  "homepage": "https://github.com/sdockray/dat-collections#README",
  "dependencies": {
    "bluebird": "^3.4.7",
    "dat-storage": "^1.0.0",
    "hyperdiscovery": "^1.3.0",
    "hyperdrive": "^9.1.0",
    "lodash": "^4.17.4",
    "pauls-dat-api": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
